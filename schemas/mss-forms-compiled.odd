          <elementSpec rend="change" module="core" ident="item">
            <gloss xmlns:teix="http://www.tei-c.org/ns/Examples" xml:lang="en"
              versionDate="2017-06-04">item</gloss>
            <gloss xmlns:teix="http://www.tei-c.org/ns/Examples" versionDate="2017-06-04"
              xml:lang="de">Listenpunkt</gloss>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples" versionDate="2005-01-14"
              xml:lang="en">contains one component of a list.</desc>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples" versionDate="2007-12-20"
              xml:lang="ko">목록에서 하나의 성분을 포함한다.</desc>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples" versionDate="2007-05-02"
              xml:lang="zh-TW">包含列表中的一個項目。</desc>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples" versionDate="2008-04-05"
              xml:lang="ja">リストのいち項目を示す．</desc>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples" versionDate="2009-01-06"
              xml:lang="fr">contient un composant d'une liste.</desc>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples" versionDate="2007-05-04"
              xml:lang="es">contiene un componente de una lista.</desc>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples" versionDate="2007-01-21"
              xml:lang="it">contiene un componente di una lista</desc>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples" versionDate="2017-06-04"
              xml:lang="de">enthält einen Listenpunkt.</desc>
            <classes xmlns:teix="http://www.tei-c.org/ns/Examples">
              <memberOf key="att.global"/>
              <memberOf key="att.sortable"/>
            </classes>
            <content xmlns:teix="http://www.tei-c.org/ns/Examples">
              <macroRef key="macro.specialPara"/>
            </content>
            <constraintSpec xmlns:teix="http://www.tei-c.org/ns/Examples" scheme="isoschematron"
              ident="item">
              <constraint>
                <sch:rule context="tei:item[ancestor::tei:collation]">
                  <sch:report test="not(@xml:id)">items in collation must have an
                    @xml:id</sch:report>
                </sch:rule>
                <sch:rule context="tei:item[ancestor::tei:collation]">
                  <sch:report test="not(matches(@xml:id,'q\d+'))"> Items in Collation must have ids
                    starting with the letter 'q' followed by a progressive number, e.g. q1, q2, q3
                  </sch:report>
                </sch:rule>
                <sch:rule context="tei:item[ancestor::tei:collation]">
                  <sch:report test="not(child::tei:dim)">Please always add a dim element inside your
                    quire descriptions</sch:report>
                  <sch:report test="child::tei:dim[not(matches(.,'\d+'))]">Dimensions of quire
                    should be given as integers.</sch:report>
                </sch:rule>
                <sch:rule context="tei:item[ancestor::tei:additions]">
                  <sch:report test="not(@xml:id)"> items in additions must have an
                    @xml:id</sch:report>
                </sch:rule>
                <sch:rule context="tei:item[ancestor::tei:additions]">
                  <sch:report test="not(matches(@xml:id,'a\d+') or matches(@xml:id,'e\d+'))">
                    Additions must have ids starting with the letter 'a' followed by a progressive
                    number, e.g. a1, a2, a3. In case of other contents (varia) use an id starting
                    with 'e. </sch:report>
                </sch:rule>
                <sch:rule context="tei:item[ancestor::tei:additions]">
                  <sch:report test="tei:desc/tei:locus[@facs]"> The locus of the addition should be
                    child of the item, not of desc. Please move it outside of the desc element, just
                    before it for example.</sch:report>
                </sch:rule>
              </constraint>
            </constraintSpec>
            <attList>
              <attDef xmlns:teix="http://www.tei-c.org/ns/Examples" ident="rend" mode="replace"
                usage="opt">
                <valList type="open">
                  <valItem ident="marginal"/>
                  <valItem ident="guardLeaf"/>
                </valList>
              </attDef>
