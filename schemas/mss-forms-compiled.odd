                  <valItem ident="note">
                    <desc>numbered note (footnote, sidenote, endnote)</desc>
                  </valItem>
                </valList>
              </attDef>
            </attList>
            <exemplum xmlns:teix="http://www.tei-c.org/ns/Examples" xml:lang="en">
              <egXML xmlns="http://www.tei-c.org/ns/Examples">
                <citedRange>pp 12–13</citedRange>
                <citedRange unit="page" from="12" to="13"/>
