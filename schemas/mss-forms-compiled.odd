    </constraint>
  </constraintSpec><attList><attDef ident="rendition" mode="delete"/><attDef ident="style" mode="delete"/><attDef ident="xml:base" mode="delete"/><attDef ident="cert" mode="delete"/><attDef ident="rend" mode="delete"/></attList><exemplum xmlns:teix="http://www.tei-c.org/ns/Examples" xml:lang="en">
