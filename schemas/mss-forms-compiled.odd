              <macroRef key="macro.phraseSeq"/>
            </content>
            <attList>
              <attDef xmlns:xi="http://www.w3.org/2001/XInclude"
                xmlns:teix="http://www.tei-c.org/ns/Examples" ident="ref" mode="change" usage="rec"/>
              <attDef xmlns:xi="http://www.w3.org/2001/XInclude"
                xmlns:teix="http://www.tei-c.org/ns/Examples" ident="role" mode="replace"
                usage="opt">
                <valList type="closed">
                  <valItem ident="illustrator">
                    <desc>the illustrator of the manuscript</desc>
                  </valItem>
                  <valItem ident="scribe">
                    <desc>the copyist of the manuscript</desc>
                  </valItem>
                  <valItem ident="donor">
                    <desc>the person who donated the manuscript</desc>
                  </valItem>
                  <valItem ident="bequeather">
                    <desc>the person who donated the manuscript, typically to a library</desc>
                  </valItem>
                  <valItem ident="author">
                    <desc>the author of a work</desc>
                  </valItem>
                  <valItem ident="translator">
                    <desc>the translator of a work</desc>
                  </valItem>
                  <valItem ident="binder">
                    <desc>manuscript binder</desc>
                  </valItem>
                  <valItem ident="parchmentMaker">
                    <desc>parchment maker</desc>
                  </valItem>
                  <valItem ident="owner">
                    <desc>the owner of a ms or an item</desc>
                  </valItem>
                  <valItem ident="patron">
                    <desc>the commissioner of a ms or an item</desc>
                  </valItem>
                  <valItem ident="sponsor">
                    <desc>the person paying for a ms (possibly not the same as
                      donor/commissioner)</desc>
                  </valItem>
                  <valItem ident="other"/>
                </valList>
              </attDef>
              <attDef xmlns:xi="http://www.w3.org/2001/XInclude"
                xmlns:teix="http://www.tei-c.org/ns/Examples" ident="type" mode="replace"
