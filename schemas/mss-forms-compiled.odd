                <sch:rule context="tei:citedRange">
                  <sch:report test="not(text())">An Empty citedRange is not allowed, remove it if
                    you don't need it or please enter the page range, unit number according to need
                    and use @unit to specify the type of range. Multiple citedRange are
                    allowed.</sch:report>
                </sch:rule>
              </constraint>
            </constraintSpec>
            <attList>
              <attDef xmlns:teix="http://www.tei-c.org/ns/Examples" ident="unit" mode="replace">
