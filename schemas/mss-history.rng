   <define name="tei_acquisition">
      <element name="acquisition">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(acquisition) Give an account of how the manuscript came into the collection where it is now held. Use the available elements to tag relevant persons, dates, etc. [10.8. History]</a:documentation>
         <ref name="tei_placeName"/>
         <ref name="tei_date"/>
         <ref name="tei_stamp"/>
         <ref name="tei_p"/>
         <ref name="tei_persName"/>
         <ref name="tei_locus"/>
         <ref name="tei_placeName"/>
         <ref name="tei_date"/>
         <ref name="tei_stamp"/>
         <ref name="tei_p"/>
         <ref name="tei_persName"/>
         <ref name="tei_locus"/>
         <ref name="tei_placeName"/>
         <ref name="tei_date"/>
         <ref name="tei_stamp"/>
         <ref name="tei_p"/>
         <ref name="tei_persName"/>
         <ref name="tei_locus"/>
         <ref name="tei_placeName"/>
         <ref name="tei_date"/>
         <ref name="tei_stamp"/>
         <ref name="tei_p"/>
         <ref name="tei_persName"/>
         <ref name="tei_locus"/>
      </element>
   </define>
   <define name="tei_model.persNamePart">
