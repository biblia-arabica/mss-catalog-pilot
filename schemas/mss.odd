                    <attList>
                        <attDef ident="xml:id" mode="change" usage="req"/>
                            <attDef ident="type" mode="change" usage="opt">
                                <datatype maxOccurs="6">
                                    <dataRef key="teidata.word"/>
                                </datatype>
                                <valList type="closed">
                                    <valItem ident="supplication"></valItem>
                                    <valItem ident="expanded"></valItem>
