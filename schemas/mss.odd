                <!--          require @xml:id-->
                <elementSpec ident="msPart" mode="change" module="msdescription">
                    <constraintSpec scheme="isoschematron" ident="msPart">
                        <constraint>
                            <sch:rule context="tei:msPart">
                                <sch:report 
                                    test="not(matches(@xml:id,'p\d+'))"> Manuscript parts
                                    ids must start with the letter 'p' followed by a progressive
                                    number, e.g. p1, p1.3, p1.1_i1.1 </sch:report>
                                <sch:report
                                    test="matches(@xml:id,'(_.*){2}')"> The Underscore is
                                    used in ids to mark a nesting of different ids, especially items
                                    in parts. there should thus be only one of them as a maximum. If
                                    you have parts into parts or items into items, please use a full
                                    stop. p1.2_i3.4 is e.g. the subitem 4 of item 3 in subpart 2 of
                                    part 1. </sch:report>
                            </sch:rule>
                        </constraint>
                    </constraintSpec>
                    <attList>
                        <attDef ident="xml:id" mode="change" usage="req"/>
                    </attList>
                </elementSpec>

                <!--          require @xml:id-->
                <elementSpec ident="msFrag" mode="change" module="msdescription">
                     <constraintSpec scheme="isoschematron" ident="msfrag">
                        <constraint>
                            <sch:rule context="tei:msFrag">
                                <sch:report
                                    test="not(matches(@xml:id,'f\d+'))"> Manuscript fragment
                                    ids must start with the letter 'f' followed by a progressive
                                    number, e.g. p1, p1.3, p1.1_i1.1 </sch:report>
                                <sch:report 
                                    test="matches(@xml:id,'(_.*){2}')"> The Underscore is
                                    used in ids to mark a nesting of different ids, especially items
                                    in parts. there should thus be only one of them as a maximum. If
                                    you have parts into parts or items into items, please use a full
                                    stop. f1.2_i3.4 is e.g. the subitem 4 of item 3 in subpart 2 of
                                    part 1. </sch:report>
                            </sch:rule>
                        </constraint>
                    </constraintSpec>
                    <attList>
                        <attDef ident="xml:id" mode="change" usage="req"/>
                    </attList>
                </elementSpec>

                <elementSpec ident="msItem" mode="change" module="msdescription">
                    <constraintSpec scheme="isoschematron" ident="msItem">
                        <constraint>
                            <sch:rule context="tei:msItem">
                                <sch:report
                                    test="not(matches(@xml:id,'_i\d+'))"> Content Items in
                                    Manuscript parts must have ids starting with the reference to
                                    the part in which they are contained, then underscore and the
                                    letter 'i' followed by a progressive number, e.g. p1.1_i1.1,
                                    p1.1_i2. </sch:report>
                                <sch:report 
                                    test="matches(@xml:id,'(_.*){2}')"> The Underscore is
                                    used in ids to mark a nesting of different ids, especially items
                                    in parts. there should thus be only one of them as a maximum. If
                                    you have parts into parts or items into items, please use a full
                                    stop. p1.2_i3.4 is e.g. the subitem 4 of item 3 in subpart 2 of
                                    part 1. </sch:report>
                                <sch:report 
                                    test="count(tei:title) gt 1"
                                    >
                                    Only one title element is allowed for each msItem.
                                </sch:report>
                            </sch:rule>
                        </constraint>
                    </constraintSpec>
                    
                    <attList>
                        <attDef ident="xml:id" mode="change" usage="req"/>
                    </attList>
                </elementSpec>

                <elementSpec ident="colophon" mode="change" module="msdescription">
                    <content>
