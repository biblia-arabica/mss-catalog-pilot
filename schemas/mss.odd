                    </attList>
                </elementSpec>
                
                <elementSpec ident="collation" module="msdescription" mode="change">
                    <attList>
                        <attDef ident="mainStructures">
                            <desc>Specifies the typical quire structure(s) of the book,
                                according to English notation (i.e. the number of folios in the
                                original quire), e.g. <val>8</val>, <val>6 8</val>. Note: This
                                is a customization which is not currently part of the TEI P5
                                standard.</desc>
                            <datatype minOccurs="1" maxOccurs="unbounded">
                                <dataRef key="teidata.count"/>
                            </datatype>
                        </attDef>
                    </attList>
                </elementSpec>
                
                <!-- make the signatures element member of att.typed
                    (this is to allow distinction between quire signatures (or numbers) and leaf/bifolium signatures) -->
                <elementSpec ident="signatures" module="msdescription" mode="change">
                    <classes mode="change">
                        <memberOf key="att.typed"/>
                    </classes>
                </elementSpec>
                
                <!-- make the foliation element a member of att.datable (this is to enable identification of medieval foliation, if desired -->
                <elementSpec ident="foliation" module="msdescription" mode="change">
                    <classes mode="change">
                        <memberOf key="att.datable"/>
                    </classes>
                </elementSpec>

                <!-- add new attributes "topLine" and "rulingMedium" to layout
                        make layout part of att.typed  -->
                <elementSpec ident="layout" module="msdescription" mode="change">
                    <classes mode="change">
                        <memberOf key="att.typed"/>
                    </classes>
                    <attList>
                        <attDef ident="topLine" mode="add">
                            <desc>Specifies the whether the writing is above top line, below top
                                line, or mixed. Note: This is a customization which is not
                                currently part of the TEI P5 standard.</desc>
                            <valList type="closed">
                                <valItem ident="above">
                                    <desc>The writing is above top line.</desc>
                                </valItem>
                                <valItem ident="below">
                                    <desc>The writing is below top line.</desc>
                                </valItem>
                                <valItem ident="mixed">
                                    <desc>The writing is variously above and below top line with
                                        no clear pattern.</desc>
                                </valItem>
                                
                            </valList>
                        </attDef>
                        <attDef ident="rulingMedium" mode="add">
                            <desc>Specifies the medium <!-- OK? --> used to carry out the
                                ruling. Note: This is a customization which is not currently
                                part of the TEI P5 standard.</desc>
                            <datatype minOccurs="1" maxOccurs="1">
                                <dataRef key="teidata.enumerated"/>
                            </datatype>
                            <valList type="semi">
                                <valItem ident="ink">
                                    <desc>The ruling is carried out using ink.</desc>
                                </valItem>
                                <valItem ident="leadpoint">
                                    <desc>The ruling is carried out using leadpoint or
                                        plummet.</desc>
                                </valItem>
                                <valItem ident="hardpoint">
                                    <desc>The ruling is carried out using hardpoint or
                                        drypoint.</desc>
                                </valItem>
                                <valItem ident="crayon">
                                    <desc>The ruling is carried out using ‘crayon’.</desc>
                                </valItem>
                                <valItem ident="mixed">
                                    <desc>The ruling is carried out using several
                                        methods.</desc>
                                </valItem>
                                <valItem ident="board">
                                    <desc>The ruling is carried out using a board.</desc>
                                </valItem>
                                
                            </valList>
                        </attDef>
                    </attList>
                </elementSpec>
                
                <!-- add new attribute value "offsite" for the availability element -->
                <elementSpec ident="availability" module="header" mode="change">
                    <attList>
                        <attDef ident="status" mode="replace">
                            <desc>Specifies the availability of a manuscript. This attribute
                                should not be used to specify the availability of manuscript
                                metadata: use the <gi>licence</gi> element for that
                                purpose.</desc>
                            <datatype maxOccurs="unbounded">
                                <dataRef key="teidata.word"/>
                            </datatype>
                            <valList type="closed">
                                <valItem ident="free">
                                    <desc>The item is available.</desc>
                                </valItem>
                                <!-- nb: not necessarily permanently restricted -->
                                <valItem ident="restricted">
                                    <desc>Access to the item is restricted for conservation or
                                        other reasons.</desc>
                                </valItem>
                                <valItem ident="exhibition">
                                    <desc>The item is on exhibition at the owning library or
                                        another institution, and is likely to be temporarily
                                        unavailable.</desc>
                                </valItem>
                                <valItem ident="offsite">
                                    <desc>The item is stored offsite, and will take at least
                                        twenty-four hours to produce.</desc>
                                </valItem>
                                <valItem ident="unknown">
                                    <desc>Availability of the item is unknown.</desc>
                                </valItem>
                            </valList>
                        </attDef>
                    </attList>
                </elementSpec>
                
                <!-- add a new element 'countermark'. Model should be same as for 'watermark'  -->
                <elementSpec ident="countermark" mode="add"
                    ns="https://github.com/msdesc/consolidated-tei-schema">
                    <desc>Contains a description of a countermark. Note: This is a customization
                        which is not currently part of the TEI P5 standard.</desc>
                    <classes>
                        <memberOf key="model.pPart.msDesc"/>
                        <memberOf key="att.global"/>
                        <memberOf key="att.global.rendition"/>
                        <memberOf key="att.global.linking"/>
                        <memberOf key="att.global.analytic"/>
                        <memberOf key="att.global.change"/>
                        <memberOf key="att.global.responsibility"/>
                        <memberOf key="att.global.source"/>
                    </classes>
                    <content>
                        <macroRef key="macro.phraseSeq"/>
                    </content>
                </elementSpec>
                
                
                <!-- use ENRICH values for supportDesc/@material and make the attribute required -->
                <elementSpec ident="supportDesc" module="msdescription" mode="change">
                    <constraintSpec ident="textLang.check" scheme="schematron">
                        <constraint>
                            <sch:rule 
                                see="https://msdesc.github.io/consolidated-tei-schema/msdesc.html#supportdesc"
                                context="//tei:sourceDesc/tei:msDesc[not(tei:msPart)]">
                                <sch:assert 
                                    test="tei:physDesc/tei:objectDesc/tei:supportDesc[@material]" 
                                    role="warn">
                                    The material (parchment, paper, etc.) of a manuscript should be recorded 
                                    in the supportDesc element, with a material attribute.
                                </sch:assert>
                            </sch:rule>
                            <sch:rule 
                                context="//tei:sourceDesc/tei:msDesc/tei:msPart">
                                <sch:assert 
                                    test="
                                    tei:physDesc/tei:objectDesc/tei:supportDesc[@material]
                                    or ancestor::tei:msDesc/tei:physDesc/tei:objectDesc/tei:supportDesc[@material]
                                    " 
                                    role="warn">
                                    The material (parchment, paper, etc.) of each part should be recorded in 
                                    the supportDesc element, with a material attribute, unless it has been 
                                    described for the entire manuscript.
                                </sch:assert>
                            </sch:rule>
                        </constraint>
                    </constraintSpec>
                    <attList>
                        <attDef ident="material" mode="replace" usage="rec">
                            <desc>The material composing the majority of the support</desc>
                            <datatype maxOccurs="1">
                                <dataRef key="teidata.word"/>
                            </datatype>
                            <valList type="semi">
                                <valItem ident="perg">
                                    <desc>The object is composed of parchment or vellum</desc>
                                </valItem>
                                <valItem ident="chart">
                                    <desc>The object is composed of any kind of paper. Give more details, 
                                        if any, in a child <gi>support</gi> element.</desc>
                                </valItem>
                                <valItem ident="papyrus">
                                    <desc>The object is composed of papyrus.</desc>
                                </valItem>
                                <valItem ident="palm">
                                    <desc>The object is composed of palm leaves.</desc>
                                </valItem>
                                <valItem ident="mixed">
                                    <desc>The object is composed of a combination of paper and
                                        parchment. The nature of the combination should be
                                        specified in the <gi>support</gi> element.</desc>
                                </valItem>
                                <valItem ident="other">
                                    <desc>The object is composed of another material (e.g.
                                        wood).</desc>
                                </valItem>
                                <valItem ident="unknown">
                                    <desc>The material of which is object is composed is not
                                        known.</desc>
                                </valItem>
                            </valList>
                        </attDef>
                    </attList>
                </elementSpec>
                

                <classSpec type="atts" ident="att.dimensions" mode="change">
                    <attList>
                        <attDef ident="precision" mode="replace">
                            <valList type="closed">
                                <valItem ident="low"/>
                                <valItem ident="medium"/>
                                <valItem ident="high"/>
                            </valList>
                        </attDef>

                    </attList>
                </classSpec>

                <classSpec type="atts" ident="att.responsibility" mode="change">
                    <attList>
                        <attDef ident="cert" mode="replace">
                            <valList type="closed">
                                <valItem ident="low"/>
                                <valItem ident="high"/>
                            </valList>
                        </attDef>
                    </attList>
                </classSpec>



            </schemaSpec>
            </div>
        </body>
    </text>
</TEI>
